{
  "name": "@pcode-at/tinacms-seo",
  "version": "2.0.0",
  "description": "SEO package for TinaCMS",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "prepublish": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcode-at/tinacms-seo.git"
  },
  "author": "@pcode-at",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pcode-at/tinacms-seo/issues"
  },
  "homepage": "https://github.com/pcode-at/tinacms-seo#readme",
  "release": {
    "branches": [
      "main",
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "style",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@amanda-mitchell/semantic-release-npm-multiple",
        {
          "registries": {
            "github": {},
            "public": {}
          }
        }
      ],
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@amanda-mitchell/semantic-release-npm-multiple": "3.8.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "prettier": "3.3.3",
    "semantic-release": "19.0.5"
  },
  "peerDependencies": {
    "next-seo": "6.0.0",
    "tinacms": "1.4.3"
  }
}
